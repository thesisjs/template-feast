{
  "name": "@thesis/feast-template",
  "version": "1.0.0",
  "description": "Feast template parser",
  "main": "src/index.ts",
  "scripts": {
    "test": "../node_modules/.bin/jest"
  },
  "author": "Kaibito Young",
  "license": "MIT",
  "jest": {
    "testRegex": "\\.tests\\.[tj]sx?$",
    "testURL": "https://localhost/",
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "coverageReporters": [
      "html", "text"
    ],
    "coverageDirectory": "report",
    "collectCoverageFrom": [
      "src/*.[tj]sx?$"
    ]
  }
}
